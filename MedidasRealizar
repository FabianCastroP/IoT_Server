
//Mean
float calculateMean(float data[]){
  float mean, sum = 0.0;
  
  for(i = 0; i < 10; ++i) {
    sum += data[i];
  }

  //Calculate mean
  mean = sum / 10;

  return mean;
}

//Max

//Minimo


//standard deviation
float calculateSD(float data[]) {
  float mean, standardDeviation = 0.0;
  int i;

  mean = calculateMean(data);

  for(i = 0; i < 10; ++i) {
    standardDeviation += pow(data[i] - mean, 2);
  }

  return sqrt(standardDeviation / 10);
}
